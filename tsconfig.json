{
  "compilerOptions": {
     "lib": [
      "ES2020"
     ],
    "target": "es2020",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "moduleResolution": "NodeNext",
    "outDir": "server/build"
  },
  "ts-node": {
    "esm": true
  },
  "exclude": [
    "node_modules",
    "build",
    "client"
  ]
}
