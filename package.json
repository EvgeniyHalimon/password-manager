{
    "name": "password-generator",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build:server": "npx tsc",
        "dev": "cd client && npm run start",
        "lint:client": "eslint \"client/**/*.{ts,tsx}\"",
        "lint:server": "eslint \"server/**/*.{ts,tsx}\"",
        "lint:fix-client": "eslint \"client/**/*.{ts,tsx}\" --fix",
        "lint:fix-server": "eslint \"server/**/*.{ts,tsx}\" --fix",
        "prepare": "husky install ./husky",
        "server": "cd server && nodemon --exec ts-node server.ts"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@typescript-eslint/parser": "^5.48.2",
        "concurrently": "^7.0.0",
        "eslint": "^8.32.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.5.2",
        "eslint-plugin-html": "^7.1.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-react": "^7.31.11",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^8.0.2",
        "nodemon": "^2.0.15",
        "typescript": "^4.8.2"
    },
    "dependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cookie": "^0.5.1",
        "@types/cookie-parser": "^1.4.3",
        "@types/express": "^4.17.15",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/mongoose": "^5.11.97",
        "@types/node": "^18.11.18",
        "@types/react-router-dom": "^5.3.3",
        "axios": "^1.3.2",
        "bcrypt": "^5.1.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "date-fns": "^2.29.3",
        "dayjs": "^1.11.7",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-validation": "^4.1.0",
        "jsonwebtoken": "^9.0.0",
        "jwt-decode": "^3.1.2",
        "mongoose": "^6.8.4",
        "ts-node": "^10.9.1",
        "uuid": "^9.0.0"
    }
}
